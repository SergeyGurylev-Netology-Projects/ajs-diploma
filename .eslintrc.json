{
    "env": {
        "browser": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "arrow-parens": "off",
        "no-continue": "off",
        "no-plusplus": "off",
        "no-else-return": "off",
        "no-underscore-dangle": "off",
        "no-mixed-operators": "off",
        "object-shorthand": "off",
        "no-restricted-syntax": "off",
        "max-len": "warn"
    }
}
